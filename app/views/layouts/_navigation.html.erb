<header>
  <% if user_signed_in?%>
    <p class= "heading">Hello <%= current_user.name%></p>
  <nav class="navbar">
    <ul>
      <li><%= link_to 'Sign Out', destroy_user_session_path, :method => "delete", :class => "links"%></li>
  <%else%>
    <li><%= link_to 'Log In', new_user_session_path, :class => "links" %></li>
    <li><%= link_to 'Sign Up', new_user_registration_path, :class => "links" %></li>
    </ul>
  </nav>
<%end%>
</header>